@model CRUD_Alumnos.Models.Docente

@{
    ViewBag.Title = "Detallesdocente";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detallesdocente</h2>

<div>
    <h4>Docente</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre Completo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreCompleto)
        </dd>

        <dt>
            Edad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Edad)
        </dd>

        <dt>
            Sexo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sexo)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Editardocente", new { id = Model.Id_Docente }) |
    @Html.ActionLink("Regresar", "Index")
</p>
